{
  "rewrites": [
    {
      "source": "/",
      "destination": "https://hongyelim.vercel.app/api"
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}
